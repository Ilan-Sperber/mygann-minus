{
  "extends": [
    "airbnb-base",
    "plugin:react/recommended"
  ],
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "globals": {
    "chrome": false
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "config/webpack.common.js"
      }
    },
    "react": {
      "pragma": "createElement"
    }
  },
  "rules": {
    "func-names": 0,
    "space-before-function-paren": 0,
    "padded-blocks": 0,
    "consistent-return": 0,
    "no-else-return": 0,
    "arrow-body-style": 0,
    "array-callback-return": 0,
    "max-len": [
      1,
      {
        "ignoreComments": true,
        "code": 100,
        "ignoreTemplateLiterals": true
      }
    ],
    "semi": 1,
    "no-param-reassign": 0,
    "prefer-const": 0,
    "no-case-declarations": 0,
    "no-unused-vars": 1,
    "object-curly-spacing": 1,
    "comma-dangle": 1,
    "linebreak-style": 0,
    "no-underscore-dangle": 0,
    "arrow-parens": 0,
    "no-nested-ternary": 0,
    "no-plusplus": 0,
    "no-await-in-loop": 0,
    "no-restricted-syntax": 0,
    "import/no-named-as-default-member": 0,
    "guard-for-in": 0,
    "no-prototype-builtins": 0,
    "no-continue": 0,
    "import/order": [
      1,
      {
        "newlines-between": "ignore"  
      }
    ],
    "react/no-unknown-property": [2, {
      "ignore": ["autocomplete"]
    }],
    "react/jsx-key": 0,
    "react/prop-types": 0,
  }
}