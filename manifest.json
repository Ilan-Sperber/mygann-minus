{

  "manifest_version": 2,
  "name": "Gann OnCampus+",
  "version": "0.5.1",
  
  "description": "A collection of modules that improve your Gann OnCampus experience",
  "version_name": "0.5.1-beta",
  
  "content_scripts": [
    {
      "matches": ["https://gannacademy.myschoolapp.com/app/*"],
      "js": ["dist/content-script.js"]
    }
   ],

   "background": {
     "scripts": ["dist/install-watch.js"],
     "persistent": false
   },

  "permissions": [
    "storage"
  ],

  "web_accessible_resources": [
    "dist/assets/*"
  ]

}
