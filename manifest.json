{

  "manifest_version": 2,
  "name": "Gann OnCampus+",
  "version": "0.5.0",
  
  "description": "A suite of modules that enhance your Gann OnCampus experience",
  
  "content_scripts": [
    {
      "matches": ["https://gannacademy.myschoolapp.com/*"],
      "js": ["dist/index.js"]
    }
   ],

  "permissions": [
    "storage"
  ],

  "web_accessible_resources": [
    "assets/*"
  ]

}
